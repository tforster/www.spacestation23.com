    <div class="site-blocks-cover" style="background-image: url(images/spacestation23-hero.jpg);" data-aos="fade"
      data-stellar-background-ratio="0.5">
      <div class="container">
        <div class="row align-items-center justify-content-center text-center">

          <div class="col-md-8" data-aos="fade-up" data-aos-delay="400">
            {{!-- <img src="images/albums/{{latestEpisode.Image}}" class="" style="width:150px">
            </img> --}}

            <h2 class="text-white font-weight-light mb-2 display-4">Episode {{latestEpisode.Ep}}:
              {{latestEpisode.Title}}
            </h2>

            <div class="text-white mb-4"><span class="text-white-opacity-05"><small>By Boo Daley |
                  {{latestEpisode.airDate}} | {{latestEpisode.Runtime}} </small></span>
            </div>

            <div class="player">
              <audio id="playerMain" preload="none" controls style="max-width: 100%">
                <source src="{{latestEpisode.URL}}" type="audio/mp3">
              </audio>
            </div>

          </div>


        </div>
      </div>
    </div>

    <!-- Recent Episodes -->
    <div class="site-section bg-light">
      <div class="container">

        <div class="row mb-5" data-aos="fade-up">
          <div class="col-md-12 text-center">
            <h2 class="font-weight-bold text-black">Recent Episodes</h2>
          </div>
        </div>

        {{#each recentEpisodes}}
        {{>recentEpisode.hbs}}
        {{/each}}

      </div>
    </div>


    <!-- Episodes Archives -->
    <div class="site-section">
      <div class="container">

        <div class="row mb-5" data-aos="fade-up">
          <div class="col-md-12 text-center">
            <h2 class="font-weight-bold text-black">The Archives</h2>
            <p>Missed an episode? All our episodes are available to listen to from our archive.</p>
          </div>
        </div>
        <div class="table-container" role="table" aria-label="Episode Archive">
          <div class="flex-table header" role="rowgroup">
            <div class="flex-row" role="columnheader">Ep</div>
            <div class="flex-row" role="columnheader">Air Date</div>
            <div class="flex-row" role="columnheader">Notes</div>
            <div class="flex-row" role="columnheader">Featured Artists</div>
            <div class="flex-row" role="columnheader">Listen</div>
          </div>

          {{#each archiveEpisodes}}
          {{>archiveEpisode.hbs}}
          {{/each}}

        </div>
      </div>
    </div>
    
    <!-- Behind the mic -->
