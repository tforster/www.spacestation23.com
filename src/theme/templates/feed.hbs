<?xml version="1.0" encoding="utf-8"?>
<rss xmlns:atom="http://www.w3.org/2005/Atom" xmlns:content="http://purl.org/rss/1.0/modules/content/"
  xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd"
  xmlns:googleplay="http://www.google.com/schemas/play-podcasts/1.0" xmlns:media="http://www.rssboard.org/media-rss"
  version="2.0">
  <channel>
    <title>{{title}}</title>
    <link>{{link}}</link>
    <language>en-ca</language>
    <atom:link href="https://www.spacestation23.com/feed.xml" rel="self" type="application/rss+xml">
    </atom:link>
    <copyright>{{author}}</copyright>
    <itunes:subtitle>{{subtitle}}</itunes:subtitle>
    <itunes:author>{{author}}</itunes:author>
    <itunes:summary>{{description}}</itunes:summary>
    <itunes:keywords>{{keywords}}</itunes:keywords>
    <description>{{description}}</description>
    <itunes:owner>
      <itunes:name>{{author}}</itunes:name>
      <itunes:email>{{authorEmail}}</itunes:email>
    </itunes:owner>
    <itunes:image href="{{image.url}}"></itunes:image>
    {{{categories}}}
    <itunes:explicit>{{explicit}}</itunes:explicit>
    <itunes:type>{{feedType}}</itunes:type>
    {{#each items}}
    <item>
      <title>{{title}}</title>
      <link>{{../link}}</link>
      <itunes:title>{{title}}</itunes:title>
      <itunes:author>{{../author}}</itunes:author>
      <itunes:subtitle>{{{description}}}</itunes:subtitle>
      <itunes:summary>
        <![CDATA[{{{featuredArtists}}}]]>
      </itunes:summary>
      <description>
        <![CDATA[{{{description}}}]]>
      </description>
      <itunes:image href="{{../link}}/images/albums/{{thumbnail}}"></itunes:image>
      <enclosure url="{{{url}}}" length="{{size}}" type="audio/mpeg"></enclosure>
      <itunes:duration>{{runtime}}</itunes:duration>
      <itunes:season>1</itunes:season>
      <itunes:episode>{{episode}}</itunes:episode>
      <itunes:episodeType>full</itunes:episodeType>
      <guid isPermaLink="false">{{{url}}}</guid>
      <pubDate>{{pubDate}}</pubDate>
      <itunes:explicit>{{explicit}}</itunes:explicit>
    </item>
    {{/each}}
  </channel>
</rss>
